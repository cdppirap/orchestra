{
	"name": "module2", 
	"description": "Simple model, does nothing. Has three arguments (parameter_id, start, stop) and 3 hyperparameters (a(int), b(float), c(string).", 
	"args": [
		["parameter","str"], 
		"start", 
		"stop"
	], 
	"hyperparameters": [
		["a","int"],
		["b","float"],
		["c","str"]
	], 
	"defaults": {
		"parameter":"imf", 
		"start":"2000-01-01T00:00:00", 
		"stop":"2000-01-02T00:00:00",
		"a":0,
		"b":0.5,
		"c":"abc"
	},
	"output": {
		"type": "timeseries",
		"filename":"output.csv"
	}, 
	"install": {
		"requirements_file":"requirements.txt",
		"files": ["main.py"], 
		"executable": "main"
	}
}
